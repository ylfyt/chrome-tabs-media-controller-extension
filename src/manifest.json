{
	"manifest_version": 2,
	"name": "Tabs Audio Controller",
	"version": "2.1.0",
  "commands": {
    "play-toggle": {
      "suggested_key": "Ctrl+Space",
      "description": "Play Pause Toggle"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
	"browser_action": {
		"default_popup": "index.html",
		"default_title": "Tabs",
		"default_icon": "icon.png"
	},
	"permissions": ["activeTab", "tabs", "<all_urls>"]
}
